$ENV{'BIBINPUTS'}='./LaTeX//:' . $ENV{'BIBINPUTS'}; # Allows .bib file to be in LaTeX subdirectory 
$bibtex_use=2;                                      # Make sure bibtex gets run
$pdf_mode = 1;
$pdflatex="pdflatex -interaction=nonstopmode %O %S%";
$out_dir = 'LaTeX';
@default_files = ('BufferStockTheory.tex');
